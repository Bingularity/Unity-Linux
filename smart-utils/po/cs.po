#: smart-url-usage:74
msgid "Usage: %s [--help|-h] [--id|-i channel_id_regex] [--notid|-n channel_id_regex] <channel_url_regex>\\n\\n"
msgstr "Použití: %s [--help|-h] [--id|-i regex_pro_id_kanálu] [--notid|-n regex_pro_id_kanálu] <regex_pro_url>\\n\\n"

#: smart-url-usage:75
msgid "Prints ids of channels whose URLs match with <channel_url_regex>. Channels can be filtered by id regex. \\n\\n"
msgstr "Vypíše id kanálů, jejichž URL souhlasí s <regexem_pro_url>. Kanaly mohou být filtrovány regexem popisujícím id kanálů. \\n\\n"

#: smart-update-channels:59
msgid "Usage: %s [--help|-h] [--verbose|-v] [--test|-t] <channelset>\\n\\n"
msgstr "Použití: %s [--help|-h] [--verbose|-v] [--test|-t] <sada_kanálů>\\n\\n"

#: smart-update-channels:60
msgid "Updates Smart channels belonging to <channelset> according\\nto the configuration found in %s<channelset>/\\n\\n"
msgstr "Aktualizuje kanály Smartu patřící do <sady_kanálů> podle\\nkonfigurace nalezené v %s<channelset>/\\n\\n"

